{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "comma-dangle": [
            "error",
            {
                "objects": "only-multiline",
                "functions": "only-multiline"
            }
        ],
        "no-var": 2,
        "prefer-const": 0
    }
}